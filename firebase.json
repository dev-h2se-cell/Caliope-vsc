
{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": [
    {
      "target": "caliope-vercel",
      "source": ".",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      },
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "ssrapp-caliope-lei71",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "target": "caliope-v2",
      "source": ".",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      },
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "ssrapp-caliope-lei71",
            "region": "us-central1"
          }
        }
      ]
    }
  ]
}
